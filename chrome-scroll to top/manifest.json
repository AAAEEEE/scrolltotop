{
  "name": "Scroll To Top",
  "version": "3.8",
  "manifest_version": 2,
  "description": "A plugin to scroll to top and vice versa of a window.",
  "homepage_url": "http://pratikabu.users.sourceforge.net/extensions/scrolltotop",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  
  "icons": {"16": "icons/pratikabu-stt-16.png",
      "48": "icons/pratikabu-stt-48-1.png",
      "128": "icons/pratikabu-stt-128.png"},
	
  "content_scripts": [
    { "css": [ "pratikabu-stt.css" ], "matches": ["<all_urls>"], "js": ["jquery-1.8.2.pratikabu.js", "jQueryRotate.js", "pratikabustt-browser-impl.js", "content_script.js"] }
  ],
  
