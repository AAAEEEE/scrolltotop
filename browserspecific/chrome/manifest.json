{
  "name": "Scroll To Top",
  "version": "4.3",
  "manifest_version": 2,
  "description": "Scroll to top and vice versa in a window.",
  "homepage_url": "http://pratikabu.users.sourceforge.net/extensions/scrolltotop",
  "options_page": "options/options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
